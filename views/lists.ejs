<%- include("header")  -%> 

    <div class="box" id="heading">
        <h1><%=listTitle%></h1>
    </div>
    
    <div class="box">        
        <% items.forEach(element=> { %>
        <div class="item">
            <input type="checkbox">
            <p><%=element%></p>
        </div>
        <% }); %>                
        
        <form class="item" action="/" method="post">
            <input type="text" name="itemName" placeholder="New Item" autocomplete="off">
            <button type="submit" name="button" value=<%=listTitle%>>+</button>
        </form>
    </div>

 <%- include("footer")  -%> 